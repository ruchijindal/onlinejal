package bill.utility;

import java.util.Calendar;

/**
 *
 * @author smp
 */
public class ConToSqldt {

    java.sql.Date sqldt;

    public java.sql.Date conToSql(Calendar dt) {
        // Initializing Calculation date
        sqldt = new java.sql.Date(dt.getTime().getTime());
        return sqldt;
    }
}
